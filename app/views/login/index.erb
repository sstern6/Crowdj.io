  <div id="login">
  <p>Welcome back!</p>


  <div id="login-errors">
    <%if @login_errors%>
      <p><%=@login_errors[0]%></p>
    <%end%>
  </div>

    <form method="post" action="/login">
      <input type="text" name="username" placeholder="Username">
      <input type="password" name="password" placeholder="Password">
      <input type="submit" value="Login!" class="button success">
    </form>
  </div>