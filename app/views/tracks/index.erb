<ul class="small-block-grid-3">

<%@tracks.each do |track|%>
  <li>
  <div class="row">
    <div class="small-8 columns">
      <img src="<%=track.artwork_url%>"></img>
      <%= track.title %>
      <audio controls>
         <source src=<%= track.stream_url+'?client_id='+ENV["CLIENT_ID"] %> type="audio/ogg">
      </audio>
    </div>
  </div>
  </li>
  <%end%>
</ul>

